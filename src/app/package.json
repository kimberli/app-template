{
  "name": "app",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite --port 3000",
    "build": "tsc -b && vite build",
    "db:generate": "tsx --tsconfig ./tsconfig.api.json node_modules/drizzle-kit/bin.cjs generate --name",
    "db:migrate": "tsx --tsconfig ./tsconfig.api.json node_modules/drizzle-kit/bin.cjs migrate",
    "lint": "eslint .",
    "preview": "vite preview",
    "deploy:staging": "npm run build -- --mode staging && wrangler deploy -c wrangler.jsonc",
    "deploy:prod": "npm run build -- --mode prod && wrangler deploy -c wrangler.jsonc",
    "test": "vitest --run --no-file-parallelism",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@axiomhq/js": "^1.3.1",
    "@axiomhq/logging": "^0.1.2",
    "@fontsource/noto-sans": "^5.2.6",
    "@heroui/react": "2.8.2",
    "@heroui/theme": "^2.4.20",
    "@hono/node-server": "^1.8.2",
    "@hono/zod-validator": "^0.4.3",
    "@supabase/ssr": "^0.5.1",
    "@supabase/supabase-js": "^2.49.4",
    "@tanstack/react-query": "^5.66.0",
    "@tanstack/react-query-persist-client": "^5.84.2",
    "axios": "^1.7.9",
    "drizzle-orm": "^0.44.4",
    "framer-motion": "^11.11.7",
    "hono": "^4.7.5",
    "hono-openapi": "^0.4.6",
    "idb-keyval": "^6.2.2",
    "postgres": "^3.4.5",
    "posthog-js": "^1.223.3",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-icons": "^5.0.1",
    "react-router-dom": "^7.5.0",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.24.2",
    "zod-openapi": "^4.2.4",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.11.2",
    "@cloudflare/workers-types": "^4.20250214.0",
    "@electric-sql/pglite": "^0.2.15",
    "@eslint/js": "^9.21.0",
    "@local/eslint-local-rules": "file:api/eslint-local-rules",
    "@tailwindcss/postcss": "^4.1.11",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.1.11",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.14.0",
    "@types/pg": "^8.11.10",
    "@types/react": "^18.2.61",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "@typescript-eslint/utils": "^8.28.0",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-no-relative-import-paths": "^1.5.5",

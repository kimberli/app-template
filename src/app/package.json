    "postcss": "^8.5.3",
    "tailwindcss": "^4.1.11",
    "tsx": "^4.7.1",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "uuid": "^11.0.5",
    "vite": "^6.2.0",
    "vite-plugin-svgr": "^4.3.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.6",
    "wrangler": "^4.28.1"
  }
